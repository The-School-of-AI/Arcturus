{
  "name": "arcturus",
  "productName": "Arcturus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"vite --host\" \"cd .. && uv run api.py\" --names \"FRONT,BACK\" --prefix-colors \"cyan,magenta\"",
    "electron:dev": "concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:5173 && electron .\" --names \"VITE,ELECTRON\" --prefix-colors \"cyan,blue\"",
    "electron:dev:all": "powershell -Command \"Get-NetTCPConnection -LocalPort 8000 -ErrorAction SilentlyContinue | ForEach-Object { Stop-Process -Id $_.OwningProcess -Force }\" ; npm run electron:dev",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:security": "vitest run src/__tests__/security.test.ts",
    "test:all": "vitest run src/__tests__"
  },
  "dependencies": {
